<header>
  <div class="container">
    <%= link_to 'New', root_path%>
    <h1> <%= title 'Edit' %></h1>
    <%= link_to 'All', all_path%>
    <h5>Edit</h5>
  </div>
</header>

<%= render 'createnote' %>

<footer>
  <div class="container">
    <div class="actions">
      <%= link_to 'Random Note', random_path%>
      <p>Created <%= time_ago_in_words @note.created_at %> ago</p>
      <%= link_to "Delete this note", note_path(@note), method: :delete, data: {confirm: "Are you sure??"}%>
    </div>
  </div>
</footer>
